{
    "manifest_version": 3,
    "name": "Gmail Email Checker",
    "version": "1.0",
    "description": "Ajoute un bouton Check à côté de chaque email.",
    "permissions": [
    "activeTab",
    "scripting"
],
    "background": {
    "service_worker": "background.js"
},
    "content_scripts": [
    {
        "matches": ["https://mail.google.com/*"],
        "js": ["contentScript.js"]
    }
]
}
